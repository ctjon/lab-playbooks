# {{ ansible_managed }}

LABEL Main Menu
        MENU LABEL Main Menu
        KERNEL menu.c32
        APPEND pxelinux.cfg/default

LABEL Linux Menu
        MENU LABEL Linux Menu
        KERNEL menu.c32
        APPEND pxelinux.cfg/Linux/LinuxMenu

#
# Fedora 34
#
label Fedora-34-x86_64
  menu label Install Fedora-34-x86_64
  kernel images/fedora/34/x86_64/vmlinuz
  append initrd=images/fedora/34/x86_64/initrd.img net.ifnames=0 biosdevname=0 noipv6 keymap=us lang=en_US bootdev=eth0 ip=dhcp inst.stage2={{linux_install_server_url}}/fedora/releases/34/Server/x86_64/os inst.repo={{linux_install_server_url}}/fedora/releases/34/Server/x86_64/os

label Fedora-34-x86_64-rescue
  menu label Rescue Fedora-34-x86_64 system
  kernel images/fedora/34/x86_64/vmlinuz
  append initrd=images/fedora/34/x86_64/initrd.img net.ifnames=0 biosdevname=0 rescue

#
# test-fedora34 server
#
label test-fedora34
  menu label Install Fedora 34 Test Server
  kernel images/fedora/34/x86_64/vmlinuz
  append initrd=images/fedora/34/x86_64/initrd.img inst.stage2={{linux_install_server_url}}/fedora/releases/34/Server/x86_64/os net.ifnames=0 biosdevname=0 noipv6 bootdev=eth0 ip=dhcp inst.ks={{linux_install_server_url}}/ks-configs/test-fedora34-ks.cfg 

