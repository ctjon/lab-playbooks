<?xml version="1.0" standalone='no'?><!--*-nxml-*-->
<!DOCTYPE service-group SYSTEM "avahi-service.dtd">
<service-group>
 <name replace-wildcards="yes">%h</name>
  <service>
   <type>_afpovertcp._tcp</type>
   <port>548</port>
  </service>
  <service>
   <type>_device-info._tcp</type>
   <port>0</port>
   <txt-record>model=Xserve</txt-record>
  </service>
  <service>
   <type>_adisk._tcp</type>
   <port>9</port>
   <txt-record>sys=waMa=0,adVF=0x100,adVU=caf82045-3147-48cf-ab29-1df6af04c38c</txt-record>
{% for tm_user in tm_users %}
   <txt-record>dk0=adVN={{tm_user.tm_username}}-TM,adVF=0x81</txt-record>
{% endfor %}
  </service>
</service-group>
