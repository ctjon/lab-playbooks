[rpmfusion-free-{{linux_install_server_hostname}}]
name=RPM Fusion for Fedora $releasever - Free
baseurl=http://install.internal.tjon.net/Linux/rpm-fusion/free/fedora/releases/$releasever/Everything/$basearch/os/
#metalink=https://mirrors.rpmfusion.org/metalink?repo=free-fedora-$releasever&arch=$basearch
enabled=1
metadata_expire=14d
type=rpm-md
gpgcheck=1
repo_gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-rpmfusion-free-fedora-$releasever

